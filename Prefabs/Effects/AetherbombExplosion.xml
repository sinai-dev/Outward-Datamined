<?xml version="1.0" encoding="utf-8"?>
<DM_StatusEffect xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <PresetID>612</PresetID>
  <Identifier>AetherbombExplosion</Identifier>
  <Name>Aetherbomb</Name>
  <Description>You will take severe Ethereal Damage after 15 seconds.</Description>
  <Lifespan>2</Lifespan>
  <LengthType>Short</LengthType>
  <RefreshRate>2</RefreshRate>
  <BuildupRecoverySpeed>3</BuildupRecoverySpeed>
  <IgnoreBuildupIfApplied>false</IgnoreBuildupIfApplied>
  <DisplayedInHUD>true</DisplayedInHUD>
  <IsHidden>true</IsHidden>
  <RemoveRequiredStatus>false</RemoveRequiredStatus>
  <NormalizeDamageDisplay>false</NormalizeDamageDisplay>
  <IgnoreBarrier>false</IgnoreBarrier>
  <Tags />
  <Effects>
    <DM_EffectTransform>
      <TransformName>Effects</TransformName>
      <Effects>
        <DM_Effect xsi:type="DM_ShootBlast">
          <Delay>0</Delay>
          <SyncType>Everyone</SyncType>
          <OverrideCategory>None</OverrideCategory>
          <CastPosition>Character</CastPosition>
          <LocalPositionAdd>
            <x>0</x>
            <y>1.5</y>
            <z>0</z>
          </LocalPositionAdd>
          <NoAim>false</NoAim>
          <TargetType>Allies</TargetType>
          <TransformName>ShooterTransform</TransformName>
          <BaseBlast>AetherbombBlast</BaseBlast>
          <Radius>4.5</Radius>
          <RefreshTime>0.1</RefreshTime>
          <BlastLifespan>0</BlastLifespan>
          <InstantiatedAmount>1</InstantiatedAmount>
          <Interruptible>false</Interruptible>
          <MaxHitTargetCount>-1</MaxHitTargetCount>
          <AffectHitTargetCenter>false</AffectHitTargetCenter>
          <HitOnShoot>true</HitOnShoot>
          <IgnoreShooter>false</IgnoreShooter>
          <BlastEffects>
            <DM_EffectTransform>
              <TransformName>Effects</TransformName>
              <Effects>
                <DM_Effect xsi:type="DM_PunctualDamage">
                  <Delay>0</Delay>
                  <SyncType>Everyone</SyncType>
                  <OverrideCategory>None</OverrideCategory>
                  <Damage>
                    <Damages>
                      <Damage>90</Damage>
                      <Damage_Type>Ethereal</Damage_Type>
                    </Damages>
                  </Damage>
                  <Damages_AI>
                    <Damages>
                      <Damage>150</Damage>
                      <Damage_Type>Ethereal</Damage_Type>
                    </Damages>
                  </Damages_AI>
                  <Knockback>140</Knockback>
                  <HitInventory>true</HitInventory>
                  <IgnoreHalfResistances>false</IgnoreHalfResistances>
                </DM_Effect>
              </Effects>
              <EffectConditions />
              <ChildEffects />
            </DM_EffectTransform>
          </BlastEffects>
        </DM_Effect>
      </Effects>
      <EffectConditions />
      <ChildEffects />
    </DM_EffectTransform>
  </Effects>
</DM_StatusEffect>